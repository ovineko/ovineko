{
  "name": "@ovineko/clean-pkg-json",
  "version": "0.0.1",
  "description": "Zero-config tool to clean package.json before publishing and restore it after",
  "keywords": [
    "clean",
    "package.json",
    "publish",
    "npm",
    "prepack",
    "postpack"
  ],
  "homepage": "https://github.com/ovineko/ovineko/tree/main/packages/clean-pkg-json",
  "bugs": {
    "url": "https://github.com/ovineko/ovineko/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovineko/ovineko.git",
    "directory": "packages/clean-pkg-json"
  },
  "license": "MIT",
  "author": "Alexander Svinarev <shibanet0@gmail.com> (shibanet0.com)",
  "type": "module",
  "bin": {
    "clean-pkg-json": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:lib": "tsup",
    "fix": "datamitsu fix",
    "lint": "datamitsu lint",
