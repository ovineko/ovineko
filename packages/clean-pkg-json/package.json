    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@ovineko/datamitsu-config": "workspace:*",
    "@shibanet0/datamitsu-config": "0.0.1-alpha-33",
    "@types/node": "24.10.12",
    "tsup": "8.5.1",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": ">=20"
