{
  "name": "@ovineko/fastify",
  "version": "0.0.0",
  "description": "Pre-configured Fastify server with Sentry, Prometheus, OpenTelemetry, and healthcheck integrations",
  "keywords": [
    "fastify",
    "server",
    "sentry",
    "prometheus",
    "opentelemetry",
    "observability",
    "monitoring",
    "healthcheck",
    "typescript"
  ],
  "homepage": "https://github.com/ovineko/ovineko/tree/main/packages/fastify",
  "bugs": {
    "url": "https://github.com/ovineko/ovineko/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovineko/ovineko.git",
    "directory": "packages/fastify"
  },
  "license": "MIT",
  "author": "Alexander Svinarev <shibanet0@gmail.com> (shibanet0.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:example:swagger": "tsx example/buildSwagger.ts && pnpm dm fix example/swagger.json",
    "dev": "tsx watch --clear-screen=true --include \"src\" --include \"example\" example/main.ts",
    "fix": "datamitsu fix",
    "lint": "datamitsu lint"
  },
  "dependencies": {
    "@fastify/compress": "8.3.1",
    "@fastify/cors": "11.2.0",
    "@fastify/sensible": "6.0.4",
    "@fastify/static": "9.0.0",
    "@fastify/swagger": "9.7.0",
    "@fastify/swagger-ui": "5.2.5",
    "@fastify/type-provider-typebox": "6.1.0",
    "close-with-grace": "2.4.0",
    "fastify": "5.7.4",
    "fastify-metrics": "12.1.0",
    "fastify-plugin": "5.1.0",
    "http-errors": "2.0.1",
    "json-schema-ref-resolver": "3.0.0",
    "openapi-types": "12.1.3",
    "pino": "10.3.1",
    "pino-pretty": "13.1.3"
  },
  "devDependencies": {
    "@ovineko/datamitsu-config": "workspace:*",
    "@shibanet0/datamitsu-config": "0.0.1-alpha-34",
    "@types/node": "24.10.12",
    "tsup": "8.5.1"
  },
  "peerDependencies": {
    "typebox": "^1.0.81"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=20"
  }
}
