    "./kysely/helpers/postgres": {
      "types": "./dist/kysely/helpers/postgres.d.ts",
      "default": "./dist/kysely/helpers/postgres.js"
    },
    "./kysely/helpers/mysql": {
      "types": "./dist/kysely/helpers/mysql.d.ts",
      "default": "./dist/kysely/helpers/mysql.js"
    },
    "./kysely/helpers/mssql": {
      "types": "./dist/kysely/helpers/mssql.d.ts",
      "default": "./dist/kysely/helpers/mssql.js"
    },
    "./kysely/helpers/sqlite": {
      "types": "./dist/kysely/helpers/sqlite.d.ts",
      "default": "./dist/kysely/helpers/sqlite.js"
    },
    "./kysely-ctl": {
      "types": "./dist/kysely-ctl/index.d.ts",
      "default": "./dist/kysely-ctl/index.js"
    }
  },
  "bin": {
    "kysely": "./bin/kysely.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "fix": "datamitsu fix",
