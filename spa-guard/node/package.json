{
  "name": "@ovineko/spa-guard-node",
  "version": "0.0.1-alpha-26",
  "description": "Server-side HTML cache with ETag/304, pre-compression, and i18n for spa-guard",
  "keywords": [
    "spa",
    "node",
    "i18n",
    "html",
    "server-side",
    "etag",
    "cache",
    "compression"
  ],
  "homepage": "https://github.com/ovineko/ovineko/tree/main/spa-guard/node",
  "bugs": {
    "url": "https://github.com/ovineko/ovineko/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovineko/ovineko.git",
    "directory": "spa-guard/node"
  },
  "license": "MIT",
  "author": "Alexander Svinarev <shibanet0@gmail.com> (shibanet0.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./builder": {
      "types": "./dist/builder.d.ts",
      "default": "./dist/builder.js"
    }
  },
  "files": [
    "dist",
    "dist-inline",
    "dist-inline-trace"
  ],
  "scripts": {
    "build": "tsup",
    "copy:bundles": "rm -rf ./dist-inline ./dist-inline-trace && cp -r ../vite/dist-inline ./dist-inline && cp -r ../vite/dist-inline-trace ./dist-inline-trace",
    "fix": "datamitsu fix",
    "lint": "datamitsu lint",
    "prepack": "clean-pkg-json clean",
    "postpack": "clean-pkg-json restore",
    "prepublishOnly": "pnpm build && pnpm copy:bundles",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:debug": "DEBUG=1 vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/accept-negotiator": "2.0.1",
    "@ovineko/spa-guard": "workspace:*",
    "html-minifier-terser": "7.2.0"
  },
  "devDependencies": {
    "@ovineko/clean-pkg-json": "workspace:*",
    "@ovineko/datamitsu-config": "workspace:*",
    "@shibanet0/datamitsu-config": "0.0.1-alpha-34",
    "@types/html-minifier-terser": "7.0.2",
    "@types/node": "24.10.12",
    "@vitest/coverage-v8": "4.0.18",
    "tsup": "8.5.1",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "parse5": "^8"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=22.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
