{
  "name": "@ovineko/spa-guard-react",
  "version": "0.0.1-alpha-31",
  "description": "React hooks, components, and error boundaries for spa-guard",
  "keywords": [
    "spa",
    "react",
    "error-boundary",
    "chunk-load-error"
  ],
  "homepage": "https://github.com/ovineko/ovineko/tree/main/spa-guard/react",
  "bugs": {
    "url": "https://github.com/ovineko/ovineko/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovineko/ovineko.git",
    "directory": "spa-guard/react"
  },
  "license": "MIT",
  "author": "Alexander Svinarev <shibanet0@gmail.com> (shibanet0.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./error-boundary": {
      "types": "./dist/react-error-boundary/index.d.ts",
      "default": "./dist/react-error-boundary/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "fix": "datamitsu fix",
    "lint": "datamitsu lint",
    "prepack": "clean-pkg-json clean",
    "postpack": "clean-pkg-json restore",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:debug": "DEBUG=1 vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ovineko/clean-pkg-json": "workspace:*",
    "@ovineko/datamitsu-config": "workspace:*",
    "@ovineko/spa-guard": "workspace:*",
    "@shibanet0/datamitsu-config": "0.0.1-alpha-34",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.2.10",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/coverage-v8": "4.0.18",
    "happy-dom": "20.7.0",
    "tsup": "8.5.1",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "@ovineko/spa-guard": "workspace:*",
    "react": "^19"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=22.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
