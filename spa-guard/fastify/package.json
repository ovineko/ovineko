{
  "name": "@ovineko/spa-guard-fastify",
  "version": "0.0.1-alpha-28",
  "description": "Fastify plugin for spa-guard beacon endpoint and HTML cache handler with ETag/304",
  "keywords": [
    "spa",
    "fastify",
    "beacon",
    "monitoring",
    "etag",
    "cache",
    "html"
  ],
  "homepage": "https://github.com/ovineko/ovineko/tree/main/spa-guard/fastify",
  "bugs": {
    "url": "https://github.com/ovineko/ovineko/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovineko/ovineko.git",
    "directory": "spa-guard/fastify"
  },
  "license": "MIT",
  "author": "Alexander Svinarev <shibanet0@gmail.com> (shibanet0.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "fix": "datamitsu fix",
    "lint": "datamitsu lint",
    "prepack": "clean-pkg-json clean",
    "postpack": "clean-pkg-json restore",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:debug": "DEBUG=1 vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ovineko/clean-pkg-json": "workspace:*",
    "@ovineko/datamitsu-config": "workspace:*",
    "@ovineko/spa-guard": "workspace:*",
    "@ovineko/spa-guard-node": "workspace:*",
    "@shibanet0/datamitsu-config": "0.0.1-alpha-34",
    "@vitest/coverage-v8": "4.0.18",
    "tsup": "8.5.1",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "@ovineko/spa-guard": "workspace:*",
    "@ovineko/spa-guard-node": "workspace:*",
    "fastify": "^5 || ^4",
    "fastify-plugin": "^5 || ^4"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=22.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
